<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
<extension
         id="application.tracing.name"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="any"
            visible="true">
         <run
               class="org.eclipse.tracecompass.incubator.internal.rcp.ui.application.Application">
         </run>
      </application>
   </extension>
<extension
      id="application.tracing.product"
      point="org.eclipse.core.runtime.products">
   <product
         application="org.eclipse.tracecompass.incubator.rcp.application"
         name="%application.name">
      <property
            name="appName"
            value="Trace Compass Incubator Rcp">
      </property>
   </product>
</extension>
<extension
      id="r"
      point="org.eclipse.core.runtime.products">
   <product
         application="application.tracing.name"
         name="Trace Compass (Incubator)">
      <property
            name="windowImages"
            value="platform:/plugin/org.eclipse.tracecompass.rcp.branding/icons/png/tc_icon_16x16.png,platform:/plugin/org.eclipse.tracecompass.rcp.branding/icons/png/tc_icon_32x32.png,platform:/plugin/org.eclipse.tracecompass.rcp.branding/icons/png/tc_icon_48x48.png,platform:/plugin/org.eclipse.tracecompass.rcp.branding/icons/png/tc_icon_64x64.png,platform:/plugin/org.eclipse.tracecompass.rcp.branding/icons/png/tc_icon_128x128.png,platform:/plugin/org.eclipse.tracecompass.rcp.branding/icons/png/tc_icon_256x256.png">
      </property>
      <property
            name="aboutText"
            value="Trace Compass&#x0A;&#x0A;Version: 3.1.0&#x0A;&#x0A;Copyright (c) 2013, 2017 Ericsson and others.&#x0A;&#x0A;All rights reserved. This program and the accompanying materials are&#x0A;made available under the terms of the Eclipse Public License v1.0 which&#x0A;accompanies this distribution, and is available at&#x0A;http://www.eclipse.org/legal/epl-v10.html">
      </property>
      <property
            name="aboutImage"
            value="platform:/plugin/org.eclipse.tracecompass.rcp.branding/icons/tc_about.png">
      </property>
   </product>
</extension>
</plugin>
