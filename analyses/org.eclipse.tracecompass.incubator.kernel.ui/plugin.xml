<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="org.eclipse.linuxtools.lttng2.ui.views.category"
            class="org.eclipse.tracecompass.incubator.internal.kernel.ui.views.fileaccess.FileAccessByFileView"
            id="org.eclipse.tracecompass.incubator.kernel.ui.filebyfile"
            name="%view.fileaccess"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="org.eclipse.linuxtools.lttng2.ui.views.category"
            class="org.eclipse.tracecompass.incubator.internal.kernel.ui.views.contextswitch.ContextSwitchView"
            icon="icons/obj16/contextswitch.png"
            id="org.eclipse.tracecompass.incubator.kernel.ui.contextswitch"
            name="%view.contextswitch"
            restorable="true">
      </view>
       <view
            category="org.eclipse.linuxtools.tmf.ui.views.category"
            class="org.eclipse.tracecompass.incubator.kernel.ui.views.io.perprocess.IoPerProcessXYView"
            id="org.eclipse.tracecompass.incubator.kernel.ui.io.per.process.xyview"
            name="%view.ioperprocess"
            restorable="true">
      </view>
      <view
            category="org.eclipse.linuxtools.lttng2.ui.views.category"
            class="org.eclipse.tracecompass.incubator.internal.kernel.ui.views.io.diskrequests.DiskRequestsView"
            icon="icons/obj16/diskQueues.png"
            id="org.eclipse.tracecompass.analysis.os.linux.views.diskrequests"
            name="%diskrequests.view.name"
            restorable="true">
      </view>
      <view
            category="org.eclipse.linuxtools.lttng2.ui.views.category"
            class="org.eclipse.tracecompass.incubator.internal.kernel.ui.views.io.latencies.IoLatencyDensityView"
            icon="icons/obj16/density.png"
            id="org.eclipse.tracecompass.analysis.os.linux.views.io.density"
            name="%io.density.view.name"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="org.eclipse.linuxtools.lttng2.ui.views.category"
            class="org.eclipse.tracecompass.analysis.timing.ui.views.segmentstore.statistics.SegmentStoreStatisticsView"
            icon="icons/obj16/statistics_view.gif"
            id="org.eclipse.tracecompass.analysis.timing.ui.segstore.statistics:org.eclipse.tracecompass.analysis.os.linux.inputoutput"
            name="%io.stats.view.name"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.linuxtools.tmf.core.analysis">
      <output
            class="org.eclipse.tracecompass.tmf.ui.analysis.TmfAnalysisViewOutput"
            id="org.eclipse.tracecompass.incubator.kernel.ui.filebyfile">
         <analysisModuleClass
               class="org.eclipse.tracecompass.incubator.internal.kernel.core.fileaccess.FileAccessAnalysis">
         </analysisModuleClass>
      </output>
      <output
            class="org.eclipse.tracecompass.tmf.ui.analysis.TmfAnalysisViewOutput"
            id="org.eclipse.tracecompass.incubator.kernel.ui.contextswitch">
         <analysisModuleClass
               class="org.eclipse.tracecompass.analysis.os.linux.core.contextswitch.KernelContextSwitchAnalysis">
         </analysisModuleClass>
      </output>
      <output
            class="org.eclipse.tracecompass.tmf.ui.analysis.TmfAnalysisViewOutput"
            id="org.eclipse.tracecompass.incubator.kernel.ui.io.per.process.xyview">
         <analysisModuleClass
               class="org.eclipse.tracecompass.incubator.internal.kernel.core.io.IoAnalysis">
         </analysisModuleClass>
      </output>
      <output
            class="org.eclipse.tracecompass.tmf.ui.analysis.TmfAnalysisViewOutput"
            id="org.eclipse.tracecompass.analysis.os.linux.views.diskrequests">
            <analysisModuleClass
               class="org.eclipse.tracecompass.internal.analysis.os.linux.core.inputoutput.InputOutputAnalysisModule">
         </analysisModuleClass>
      </output>
      <output
            class="org.eclipse.tracecompass.tmf.ui.analysis.TmfAnalysisViewOutput"
            id="org.eclipse.tracecompass.analysis.timing.ui.segstore.statistics:org.eclipse.tracecompass.analysis.os.linux.inputoutput">
         <analysisModuleClass
               class="org.eclipse.tracecompass.internal.analysis.os.linux.core.inputoutput.InputOutputAnalysisModule">
         </analysisModuleClass>
      </output>
      <output
            class="org.eclipse.tracecompass.tmf.ui.analysis.TmfAnalysisViewOutput"
            id="org.eclipse.tracecompass.analysis.os.linux.views.io.density">
         <analysisModuleClass
               class="org.eclipse.tracecompass.internal.analysis.os.linux.core.inputoutput.InputOutputAnalysisModule">
         </analysisModuleClass>
      </output>
   </extension>
</plugin>
